<%= render PageTitle::View.new(title: "trainees.not_supported_route") %>

<%= content_for(:breadcrumbs) do %>
  <%= render GovukComponent::BackLink.new(
    text: 'Back',
    href: new_trainee_path
  ) %>
<% end %>

<div class="govuk-grid-row">
  <div class="govuk-grid-column-two-thirds">
    <h1 class="govuk-heading-l">
      Other routes not supported
    </h1>

    <p class="govuk-body">
      Assessment Only is the only route currently supported by this service. More routes will be added soon.
    </p>

    <p class="govuk-body">You can:</p>
    <ul class="govuk-list govuk-list--bullet">
      <li>
        <%= govuk_link_to "go back and change your answer", new_trainee_path  %>
      </li>
      <li>
        <%= govuk_link_to "use DTTP to register a different route", Settings.dttp.back_office_url  %>
      </li>
    </ul>
  </div>
</div>
